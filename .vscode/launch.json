{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}\\src\\index.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			 "preLaunchTask": "build-ts",
			 "runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			// "env": {
			// 	"NODE_ENV": "development"
			// },
			"externalConsole": false,
			"sourceMaps": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}